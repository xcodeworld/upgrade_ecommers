{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/Admin/index.html"
    }
  ]
}