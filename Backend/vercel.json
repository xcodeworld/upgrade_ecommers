{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "routes": [
    {
      "src": "/(*)",
      "dest": "/Frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/Admin/index.html"
    }
  ]
}